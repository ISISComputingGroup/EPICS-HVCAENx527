record( stringin, "$(P):$(PSNAME):crateIP")
{
        field(DESC, "Name of Slot0 Board")
	field(DTYP, "CAEN x527 generic HV Mainframe")
	field(SCAN, "1 second")
	field(INP,  "@$(PSNAME) -1")
	field(FLNK, "$(PSNAME):boardName0.PROC")
	field(SIML, "$(P):SIM")
	field(SIOL, "$(P):SIM:$(PSNAME):crateIP")
	field(SDIS, "$(P):DISABLE")
}
